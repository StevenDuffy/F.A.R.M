<Window x:Class="F.A.R.M.Login" Icon="Logo.png" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:F.A.R.M"
        mc:Ignorable="d"
        Title="Login" Height="300" Width="300">
    <Grid>
        <Image Source="Logo.png"></Image>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="200">
            <Label Content="Username:" FontWeight="bold"></Label>
            <TextBox Width="200"></TextBox>
            <Label Content="Password:" FontWeight="bold"></Label>
            <PasswordBox Width="200"></PasswordBox>
            <Button Content=" Submit " HorizontalAlignment="Right" Margin=" 0,10" Click="Button_Click"></Button>
        </StackPanel>
    </Grid>

</Window>
